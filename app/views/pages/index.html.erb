                      <a class="btn btn-primary no-border btn-bigger bx-layer" data-anim="slideInUp" data-dur="1000" data-delay="600">track shipment</a>
                    </div>
                    <!-- /Text -->
                    <%= image_tag ("gtauto/boat.png"), class: "img-main" %>
