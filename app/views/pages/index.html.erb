                    </div>
                    <!-- /Text -->
                    <%= image_tag ("gtauto/carYellowNew.png"), class: "img-main" %>
                  </div>
                  <!-- /Slide -->
                  
                  <!-- Slide -->
                  <div class="slide">
                    <!-- Text -->
                    <div class="text">
